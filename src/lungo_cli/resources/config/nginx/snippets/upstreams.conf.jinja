upstream oathkeeper {
    server {{ ip_addresses['oathkeeper'] }}:80;
    keepalive 4;
}

upstream node {
    server {{ ip_addresses['node'] }}:80;
    keepalive 4;
}

{% for plugin_output in plugin_outputs %}
    {% set plugin_name = plugin_output.config.name %}
    {% if plugin_output.config.have_backend and config.plugins[plugin_name].enabled %}
upstream {{ plugin_name }} {
    server {{ ip_addresses[plugin_name] }}:{{ plugin_output.config.backend_port }};
    keepalive 4;
}
    {% endif %}
{% endfor %}
